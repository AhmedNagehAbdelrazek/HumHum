@model string
<div class="container-fluid" style="height:60vh">
    <div>
        <h1 class="" style="color:var(--bs-red)">NOT FOUND PAGE</h1>
    </div>
    <div class="d-flex justify-content-between flex-column h-100">
        @if (Model != null)
        {
            <p class="h3">
                no result Found For <span class="text-white fst-italic">@Model</span>
            </p>
        }else{
            <p class="h3">no result Found</p>
        }
        <a asp-action="Index" asp-controller="Home" style="max-width:200px" class="btn btn-primary p-3">back to home</a>
    </div>
</div>